<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cook.animations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ActivityPropertyAnimations" />
        <activity android:name=".ActivityViewAnimations" />
        <activity android:name=".ActivityLayoutTransitions" />
        <activity android:name=".requestingLayout.ActivityTestRequestLayout" />
        <activity android:name=".keyframe.ActivityKeyFrame" />
        <activity android:name=".crossfading.ActivityCrossFading" />
        <activity android:name=".viewPicture.ActivityViewPicture" />
        <activity android:name=".anticipationOvershoot.ActivityAnticipationOvershoot" />
    </application>

</manifest>
